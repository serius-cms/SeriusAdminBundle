{
    "name": "serius-cms/serius-admin-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 bundle that extends Sonata Admin, providing a CMS backoffice",
    "license": "MIT",
    "authors": [
        {
            "name": "Dirk Luijk",
            "email": "dirk@luijkwebcreations.nl"
        }
    ],
    "require": {
        "friendsofsymfony/jsrouting-bundle": "1.5.*",
        "sonata-project/admin-bundle": "2.3.*",
        "stof/doctrine-extensions-bundle": "1.1.*",
        "sonata-project/doctrine-orm-admin-bundle": "2.2.x",
        "knplabs/knp-menu-bundle": "2.0.*",
        "knplabs/knp-menu": "2.0.*",
        "egeloen/ckeditor-bundle": "~2.0"
    },
    "autoload": {
      "psr-4": { "Serius\\Bundle\\AdminBundle\\": "" }
    },
    "scripts": {
      "post-install-cmd": [
        "bower install"
      ],
      "post-update-cmd": [
        "bower install"
      ]
    }
}
